{
  "extends": [
    "next/core-web-vitals",
    "plugin:tailwindcss/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "requireConfigFile": false
  },
  "plugins": ["tailwindcss", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "tailwindcss/no-custom-classname": "off"
  }
}
